{
  "expo": {
    "name": "stark-pay",
    "slug": "stark-pay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robertoyamanaka.starkpay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.NFC"
      ],
      "package": "com.robertoyamanaka.starkpay"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-nfc-manager",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow StarkPay to use Face ID."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "cbcb47e3-5bb8-43a3-af07-870dad4542d0"
      }
    }
  }
}
